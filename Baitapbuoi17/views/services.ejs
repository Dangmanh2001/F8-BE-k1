<% if(services?.length) {%>
   <% services.forEach(({services_taital,services_text,decorate_text,tation_text,readmore_bt,decorate_text1,decorate_text2,link_img1,link_img2,link_img3}) => {%>
      <!-- services section start -->
      <div class="services_section layout_padding">
         <div class="container">
            <div class="row">
               <div class="col-sm-12">
                  <h1 class="services_taital"><%=  services_taital%></h1>
                  <p class="services_text"><%=  services_text%></p>
               </div>
            </div>
            <div class="services_section_2">
               <div class="row">
                  <div class="col-lg-4 col-sm-12 col-md-4">
                     <div class="box_main active">
                        <div class="house_icon">
                           <img src="<%= link_img1%>" class="image_1">
                           <img src="<%= link_img1%>" class="image_2">
                        </div>
                        <h3 class="decorate_text">Original Coffee</h3>
                        <p class="tation_text">Exerci tation ullamcorper suscipit lobortis nisl ut aliquip ex ea </p>
                        <div class="readmore_bt"><a href="#">Read More</a></div>
                     </div>
                  </div>
                  <div class="col-lg-4 col-sm-12 col-md-4">
                     <div class="box_main">
                        <div class="house_icon">
                           <img src="<%= link_img2%>" class="image_1">
                           <img src="<%= link_img2%>" class="image_2">
                        </div>
                        <h3 class="decorate_text"><%= decorate_text1%></h3>
                        <p class="tation_text"><%= tation_text %> </p>
                        <div class="readmore_bt"><a href="#"><%= readmore_bt %></a></div>
                     </div>
                  </div>
                  <div class="col-lg-4 col-sm-12 col-md-4">
                     <div class="box_main">
                        <div class="house_icon">
                           <img src="<%= link_img3%>" class="image_1">
                           <img src="<%= link_img3%>" class="image_2">
                        </div>
                        <h3 class="decorate_text"><%= decorate_text2 %></h3>
                        <p class="tation_text"><%= tation_text %></p>
                        <div class="readmore_bt"><a href="#"><%= readmore_bt %></a></div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>
      <!-- services section end -->
      <%}); %>
      <%   } %>   