<h1>Quản lý <link rel="stylesheet" href=""></h1>
<% if(msg?.length){ %>
<div class="col alert alert-success"><%= msg %></div>
<% } %>
<form action="" method="post">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th width="5%">STT</th>
        <th>Link rút gọn</th>
        <th>Link gốc</th>
        <th>Sửa</th>
        <th>Xóa</th>
      </tr>
    </thead>
    <tbody>
      <% shortLinks.forEach(({shorten,original,id}, index) => { %>

      <tr>
        <td><%= index + 1 %></td>
        <td><%= shorten %></td>
        <td><%= original %></td>


        <td>
          <a href="#" class="btn btn-warning">Sửa</a>
        </td>
        <td>
            <a href="#" class="btn btn-danger">Xóa</a>
          </td>
      </tr>
      <%}) %>
    </tbody>
  </table>
  
</form>
